/**app.wxss**/
page{background-color: #EBEEF5;}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.row {
  display: block;
  margin: 0px;  
} 
.col {
  display: flex;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
  font-size: 17px;
} 
.col>.col-1, .col-2, .col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9, .col-10, .col-11, .col-12 {
  overflow: hidden;
} 
.col-1 {
  width: 8.33333333333333%;
} 
.col-2 {
  width: 16.6666666666666%;
} 
.col-3 {
  width: 25%;
} 
.col-4 {
  width: 33.3333333333333%;
} 
.col-5 {
  width: 41.6666666666666%;
} 
.col-6 {
  width: 50%;
} 
.col-7 {
  width: 58.333333333333333%;
} 
.col-8 {
  width: 66.66666666666666%;
} 
.col-9 {
  width: 75%;
} 
.col-10 {
  width: 83.33333333333333%;
} 
.col-11 {
  width: 91.66666666666666%;
} 
.col-12 {
  width: 100%;
}
.x-center {
  text-align: center;
}
.x-right {
  text-align: right;
}
.clearfix{
  clear: both;
  overflow: hidden;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.f12{
  font-size: 12px;
}
.f14{
  font-size: 14px;
}
.f22r{
  font-size: 22rpx;
}
.f26r{
  font-size: 26rpx;
}
.show{
  display: block;
}
.hidden{
  display: none;
}
/* 底部导航样式 */
.menu-item{  
  width: 50%;  
  float: left;  
  text-align: center;  
  padding-top: 18rpx;  
  font-size: 20rpx;
  height:80rpx;
}  
.menu-item1{  
  width: 20%;  
  float: left;  
  text-align: center;  
  padding-top: 18rpx;  
  font-size: 20rpx;
  height:80rpx;
}  
.tabbar_img{ 
  width: 38rpx;  
  height: 38rpx;  
  display: block;  
  margin:auto;  
} 
.tabbar_text{
  padding: 10rpx 0 0 0;
  display: block;
} 
.clear{  
  clear: both;  
}  
.tab-bar{  
  position: fixed;
  width: 100%;
  padding: 0px;
  background-color:#F8F8F8;
  z-index: 99;
}  

/* 引入字体库 */
@font-face {
  font-family: 'iconfont';  /* project id 1859864 */
  src: url('//at.alicdn.com/t/font_1859864_ek2hge6zbxw.eot');
  src: url('//at.alicdn.com/t/font_1859864_ek2hge6zbxw.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1859864_ek2hge6zbxw.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1859864_ek2hge6zbxw.woff') format('woff'),
  url('//at.alicdn.com/t/font_1859864_ek2hge6zbxw.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1859864_ek2hge6zbxw.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

