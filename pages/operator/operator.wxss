.contain{padding-bottom: 120rpx;}
.fenlei{background:rgba(245,247,250,1);box-shadow:0px 1px 5px 0px rgba(0,0,0,0.09); height: 80rpx; line-height: 80rpx;}
.nav_text{
  text-align: center;
}
.nav_text text{
  height: 80rpx;
  display: inline-block;
}
.on{
  background-image:url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 56rpx 8rpx;
  background-position: center 60rpx;
}
.screen{
  text-align: right;
  padding-right: 10px;
  font-size: 24rpx;
}
.list_con{width: 92%; margin: 16rpx auto 0; background-color: #fff; border-radius: 4rpx; padding: 0 2% 2%;}
.list_zwh{ height: 82rpx; line-height: 82rpx;}
.list_zwh .list_zwh_left{ font-weight: bold; color: #303133; font-size: 32rpx;}
.list_zwh .list_zwh_right{color:#606266;}
.list_li .list_img{width: 96rpx; height: 96rpx; margin-right: 16rpx;}
.list_li .list_img_biao{width: 42rpx; height: 42rpx; margin-right: 16rpx;}
.list_li .list_licon{width: calc(100% - 112rpx);font-size: 30rpx;color:rgba(0,0,0,0.9);}
.list_litit{ font-weight: bold; }
.list_list{ height: 60rpx; line-height: 60rpx;}
.list_listname{color: #606266; }
.list_btn{font-size: 28rpx;height: 48rpx; line-height: 48rpx; margin-top: 2%;}
.list_btn .urging{color: #E54C4A; margin-right: 20rpx;}
.list_btn .urging .air_bg{color: #fff; background-color: #E54C4A; border-radius: 30rpx;width:40rpx; height: 40rpx; line-height: 40rpx; text-align: center;display: inline-block; margin-right: 8rpx;}
.list_btn .urging .air_bg text{ font-size: 24rpx;}
.list_btn .assign_on{width: 160rpx;  background-color: #ECBB49;border-radius:24rpx; color: #fff; text-align: center; }
.list_btn .assign{width: 160rpx;height: 44rpx; line-height: 44rpx;background:rgba(255,255,255,1);border-radius:24rpx;color:#606266;border:1px solid rgba(220,223,230,1);text-align: center; }
/* 筛选样式 */
.mask_screen{
  background-color:rgba(0,0,0,0.5);
  width: 100%;
  height: 100%;
  opacity:0.5;
  position: fixed;
  z-index: 110;
  bottom:0;  
}
.display-block{
  display:block !important;
}
.display-none{
  display: none !important;
}
.screen_area{ padding:0 20rpx; position: fixed;top:0;right: 0;width: 70%;height: 100%; background-color: #fff;z-index: 120; font-size: 28rpx;}
.screen_area .screen_tit{text-align: center; font-size: 32rpx; height: 92rpx; line-height: 92rpx;}
.screen_con{ margin-top: 20rpx;}
.screen_con02,.screen_con03{ margin-top: 40rpx;}
.screen_contit{ height: 50rpx; line-height: 50rpx;}
.screen_conli text{display:inline-block;height:64rpx;line-height:64rpx;border-radius:32rpx;background:#F4F6F8;border:1px solid #F4F6F8; margin:2%; text-align: center;}
.screen_conli text.on{background:rgba(236,187,73,0.05);border:1px solid #ECBB49;}
.screen_con .screen_conli text{width:128rpx; }
.screen_con02 .screen_conli text,.screen_con03 .screen_conli text{width:96%;}
.screen_btn{ position: absolute; bottom: 0; left:5%;height: 80rpx; line-height: 80rpx;width:90%;}
.screen_btn text{display:inline-block;width:208rpx;height:48rpx;line-height:48rpx;border-radius:24rpx; text-align: center;}
.screen_btn .reset_btn{border:1px solid rgba(220,223,230,1);}
.screen_btn .confirm_btn{border:1px solid rgba(236,187,73,1);background:rgba(236,187,73,1); color: #fff;}
/* 转单弹窗样式 */
.order_con{height: 600rpx; overflow: auto;}
.or_content{width: 100%; margin: 0 auto;}
.or_zwh{font-weight: bold; color: #303133; font-size: 36rpx; height: 60rpx; line-height: 60rpx;}
.or_img{width: 96rpx; height: 96rpx;}
.zd_con{width: calc(100% - 112rpx);}
.zd_litit{font-weight: bold;color: #303133;}
.zd_list{height: 60rpx; line-height: 60rpx;}
.zd_listname {width:calc(100% - 60rpx); text-overflow: ellipsis; white-space:nowrap; overflow:hidden; color: #606266;}
.zd_listnum {color: #303133;}

