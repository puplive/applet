.ul {
    display: flex;
    align-items: center;
    padding: 8px;
}

.ul .li {
    flex: 1;
    width: 25%;
    background: #fff;
    /* display: flex; */
    box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.1);
    border-radius: 2px;
    border: 1px solid #F4F6F8;
    padding: 8px;

}

.ul .li+.li {
    margin-left: 8px;
}

.ul .li image {
    display: block;
    width: 28px;
    height: 28px;
    margin: 8px auto;

}

.ul .li view {
