page {
    background-color: #EBEEF5;
}

.navigator-box {
    opacity: 0;
    position: relative;
    background-color: #EBEEF5;
    line-height: 1.41176471;
    font-size: 34rpx;
    transform: translateY(-50%);
    transition: .3s;
}

.navigator-box-show {
    opacity: 1;
    transform: translateY(0);
}

.navigator {
    padding: 20rpx 30rpx;
    position: relative;
    display: flex;
    align-items: center;
}

.offline {
    opacity: 0.5;
}

.navigator:before {
    content: " ";
    position: absolute;
    left: 30rpx;
    top: 0;
    right: 30rpx;
    height: 1px;
    /* border-top: 1rpx solid #D8D8D8; */
    color: #D8D8D8;
}

.navigator:first-child:before {
    display: none;
}

.navigator-text {
    flex: 1;
}

.page-tabBar{
  background: #fff;
}

