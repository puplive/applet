.contain{padding-bottom: 220rpx;}
/* 搜索 */
.search {
  position: relative;
  width: 96%;
  height:60rpx;
  line-height: 60rpx;
  padding: 0 80rpx;
  box-sizing: border-box;
  border-radius: 30rpx;
  background: #DCDFE6;
  margin: 20rpx 2%;
}
.search .icon_search {
  display: block;
  position: absolute;
  top: 50%;
  left: 30rpx;
  transform: translateY(-50%);
  width: 30rpx;
}
.search .search_input{
  font-size: 28rpx;
  height: 56rpx;
  line-height: 56rpx;
}
.search .icon_voice {
  display: block;
  position: absolute;
  top: 50%;
  right: 30rpx;
  transform: translateY(-50%);
  width: 20rpx;
}
