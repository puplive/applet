.page-inspec-detail{
    height: 100vh;
    display: flex;
    flex-direction: column;
    background: #fff;
}
.msg{
    display: flex;
    align-items: center;
    height: 50px;
    padding: 0 8px;
}
.msg .title{
    font-size: 17px;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: rgba(0,0,0,0.9);
    line-height: 24px;
}
.msg .icon{
    width: 22px;
    height: 22px;
    margin-right: 8px;
}
.msg .num{
    font-size: 17px;
    font-family: PingFangSC-Medium, PingFang SC;
    color: #D0021B;
    line-height: 24px;
}

.content{
    flex: 1;
    height: 0;
    overflow: auto;
    /* padding: 8px 0; */
}

.ul{
    padding: 8px;
}

.ul .li{
    display: flex;
    align-items: center;
    height: 50px;
    border-bottom: 1px solid #DCDFE6;
    padding-left: 10px;
}
.ul .text{
    font-size: 17px;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #000000;
    line-height: 20px;

    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;

    flex: 1;
}

.ul .icon{
    width: 14px;
    height: 14px;
    cursor: pointer;
    padding: 10px;
}
