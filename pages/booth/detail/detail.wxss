.page-booth-detail{
    height: 100vh;
    display: flex;
    flex-direction: column;
    background: #fff;
}
.content{
    flex: 1;
    height: 0;
    overflow: auto;
    padding: 8px;
}
.p1{
    font-size: 17px;
    color: rgba(0,0,0,0.9);
    line-height: 24px;
    margin-top: 10px;
    /* margin-bottom: 8px; */
}
.ul{
    margin-left: -8px;
}
.ul .li{
    margin: 8px 0 0 8px;
    width: calc(25vw - 12px);
    height: calc(25vw - 12px);
    border: 1px solid #DCDFE6;

    float: left;
}
.ul .li image{
    width: 100%;
    height: 100%;
}
.msg-box{
    padding: 10px;
    
}
.msg-box .p2{
    padding: 20px 10px;
}
.msg-box .p2+.p2{
    border-top: 1px solid #F4F6F8;
}
.msg-box .p3{
    font-size: 16px;
    color: #303133;
    line-height: 22px;
}
.msg-box .p3+.p3{
    margin-top: 8px;
}
.msg-box .p4{
    font-size: 24px;
    color: #303133;
    line-height: 22px;
    margin-top: 24px;
}

